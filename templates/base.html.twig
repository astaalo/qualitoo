<body class="dashborad">
{% block body %}
    <div id="alertMessage" class="error"></div>
    <div id="header">
        <div class="logo"></div>
        <div id="account_info">
            {# {% if has_rights('ROLE_ADMIN') or has_rights('ROLE_SUPER_ADMIN') or has_rights('ROLE_RISKMANAGER') or has_rights('ROLE_AUDITEUR') %} #}
            <div id="societe_choice">
	                	<span>{% if app.user.societe and app.user.societe.photo %}<img src="{{ asset(app.user.societe.webPath) }}" />{% endif %}
	                		<b>{{ app.user.societe?app.user.societe:"" }}</b>
	                	</span>
                {% set societes = app.user.allSocietes %}

                <span class="scroll_down_menu"></span>
                <ul style="display: none;">
                    {% for societe in societes %}
                        {# <li ><a href="#" class="ajaxLink" modal-url={{ path('changement_societe', {id: societe.id}) }} call="redirectTo" argument-call="{{ path(app.request.attributes.get('_route'), app.request.attributes.get('_route_params')) }}"> {{ societe }} </a></li> #}
                        <li ><a href="#" class="ajaxLink" modal-url={{ path('dashboard') }} call="redirectTo" argument-call="{{ path(app.request.attributes.get('_route'), app.request.attributes.get('_route_params')) }}"> {{ societe }} </a></li>
                    {% endfor %}
                </ul>

            </div>
            {# {% endif %} #}
            <div style="float: left;"><img src="{{ asset('assets/bundles/orangemain/images/logo/logo.gif') }}" alt="Online" class="avatar" /></div>
            <div class="setting" title="Modification du profil"><b>Bienvenue,</b> <b class="red">{{ app.user }}</b><img src="{{ asset('assets/bundles/orangemain/images/gear.png') }}" class="gear"  alt="Profile Setting" ></div>
            <div title="Disconnect">
                {% if is_granted("ROLE_PREVIOUS_ADMIN") %}
                    <a href="{{ path('dashboard') ~ '?_want_to_be_this_user=_exit' }}"><b >Déconnexion</b></a>
                {% else %}
                    <a href="{{ path('fos_user_security_logout') }}"><b >Déconnexion</b></a>
                {% endif %}
                <img src="{{ asset('assets/bundles/orangemain/images/connect.png') }}" name="connect" class="disconnect" alt="disconnect" >
            </div>
        </div>
    </div> <!--//  header -->
    <div id="shadowhead"></div>
    {% block menu %}{% endblock %}
    <div id="content">
        <div class="inner">
            <div class="topcolumn">
                <div class="logo"></div>
                {% block shortcut %}
                    {{ knp_menu_render('OrangeMainBundle:Builder:showShortcut', {'template': 'Extra/knp_shortcut.html.twig'}) }}
                {% endblock %}
            </div>
            <div class="clear"></div>
            <br /><br />
            {% block content %}{% endblock %}
            {% include 'Extra/flash.html.twig' %}
            <div class="modal_dialog hide" id="myModal" ></div>
            <div class="hide" id="loading-modal">
                <div class="header">
                    <span>Traitement en cours ...</span>
                    <div class="close_me"><a data-dismiss="modal"><img src="{{ asset('assets/bundles/orangemain/images/icon/closeme.png') }}"> </a></div>
                </div>
                <div class="content">
                    <div style="height: 250px">
                        <img style="margin-top: 5px;margin-left: 180px;" width="200" src="{{ asset('assets/bundles/orangemain/images/loader/black.gif') }}" />
                    </div>
                    <div class="section last">
                    </div>
                </div>
            </div>
            <div id="footer"> &copy; Copyright 2015 <span class="tip"><a  href="#" title="DSD/DASI/PMA" >SONATEL MULTIMEDIA</a> </span> </div>
        </div> <!--// End inner -->
        <div>
        </div>
    </div> <!--// End content -->
{% endblock %}
{% block script_js %}
    <script type="text/javascript">
        {% set functionAfterShowModal %}
        function() {
            activateModals();
            $('select').not("select.chzn-select,select.no-chzn,select[multiple],select#box1Storage,select#box2Storage").selectmenu({
                style: 'dropdown', transferClasses: true, width: null
            });
            $(".chzn-select").chosen({
                search_contains: true
            });
            $("#myModal").draggable({ handle: "div.header" });
            $(".on_off_checkbox").iphoneStyle();
            $("input.fileupload").filestyle();
            $("#myModal input:radio").customInput();
        }
        {% endset %}
        {% set functionOnComplete %}
        todoOnCompleteWithError();
        {% endset %}
        {{ addEventActionLink(functionAfterShowModal) }}
        {{ loadAjax() }}
        {{ formModalSubmit('#myModal', '#loading-modal', functionOnComplete) }}
        addEventActionLink();
        loadAjax();
        formModalSubmit();

        $(function(){
            $("#dropdown").juizDropDownMenu({
                'showEffect' : 'slide',
                'hideEffect' : 'slide'
            });
        });

    </script>
{% endblock %}

