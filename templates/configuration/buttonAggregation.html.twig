		{% if carto <= 2 %}
			<li><a class="uibutton type_aggreg {{ type==0 ? 'active'  : null}}" href="{{ path(app.request.attributes.get('_route'),{carto:carto, type:0}) }}">Entité</a></li>
			<li><a class="uibutton type_aggreg {{ type==1 ? 'active'  : null}}" href="{{ path(app.request.attributes.get('_route'),{carto:carto, type:1}) }}">Sous-entité</a></li>
