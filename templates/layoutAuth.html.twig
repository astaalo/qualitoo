{% trans_default_domain 'FOSUserBundle' %}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>{% block title %}CORIS ARQ{% endblock %}</title>
    <link href="{{ asset('assets/bundles/orangemain/css/zice.style.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('assets/bundles/orangemain/css/icon.css') }}" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" type="text/css" href="{{ asset('assets/bundles/orangemain/components/tipsy/tipsy.css') }}" media="all"/>
    <style type="text/css">
        html {
            background-image: none;
        }
        #versionBar {
            background-color:#212121;
            position:fixed;
            width:100%;
            height:35px;
            bottom:0;
            left:0;
            text-align:center;
            line-height:35px;
        }
        .copyright{
            text-align:center; font-size:10px; color:#CCC;
        }
        .copyright a{
            color:#A31F1A; text-decoration:none
        }
    </style>
</head>
<body >
<div id="alertMessage" class="error"></div>
<div id="successLogin"></div>
<div class="text_success"><img src="{{ asset('assets/bundles/orangemain/images/loadder/loader_green.gif') }}"  alt="ziceAdmin" /><span>Patientez ...</span></div>

<div id="login" >
    <div class="inner">
        <div  class="logo" ><span class="login_title">CORIS - ARQ</span></div>
        <div class="userbox"></div>
        <div class="formLogin">
            {% block content %}{% endblock %}
        </div>
    </div>
    <div class="clear"></div>
    <div class="shadow"></div>
</div>

<!--Login div-->
<div class="clear"></div>
<div id="versionBar" >
    <div class="copyright" > &copy; Copyright 2021  Tous droits réservés <span class="tip"><a  href="#" title="Zice Admin" >SONATEL MULTIMEDIA</a> </span> </div>
    <!-- // copyright-->
</div>
<!--[if lt IE 9]>
			<script src="{{ asset('assets/bundles/orangemain/js/trunk/html5.js') }}"></script>
		<![endif]-->
<!-- Link JScript-->
<script type="text/javascript" src="{{ asset('assets/bundles/orangemain/js/jquery.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('assets/bundles/orangemain/components/effect/jquery-jrumble.js') }}"></script>
<script type="text/javascript" src="{{ asset('assets/bundles/orangemain/components/ui/jquery.ui.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('assets/bundles/orangemain/components/tipsy/jquery.tipsy.js') }}"></script>
<script type="text/javascript" src="{{ asset('assets/bundles/orangemain/components/checkboxes/iphone.check.js') }}"></script>
<script type="text/javascript" src="{{ asset('assets/bundles/orangemain/js/login.js') }}"></script>
</body>
</html>