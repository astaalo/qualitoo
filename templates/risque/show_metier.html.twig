			<tr>
				<th style="width:140px;" >Responsable:</th>
				<td style="padding-left: 70px;">{{ entity.responsable | default('Non renseigné') }}</td>
			</tr>
			{% set processus = entity.activite.processus | default('') %}
			<tr>
				<th style="width:140px;" >Structure:</th>
				<td style="padding-left: 70px;">{{ processus.structure | default('') }} ({{ processus.structure.name | default('aucune') }})</td>
			</tr>
			<tr>
			   {% if processus is defined and processus is not empty%}
					<th style="width:140px;" >{{ processus.typeProcessus }}:</th>
					<td style="padding-left: 70px;">
					<a href="{{ path( 'details_processus', {id: processus.id}) }}">{{ processus }}</a>
					</td>
			  {% endif %}
			</tr>
			<tr>
				<th style="width:140px;" >Activité:</th>
				{% if entity.activite is defined and entity.activite is not empty%}
					<td style="padding-left: 70px;"><a href="{{ path('details_activite', {id: entity.activite.id}) }}">{{ entity.activite }}</a></td>
				{% else %}
					<td style="padding-left: 70px;">{{ identification.activite | default('aucune') }}</td>
				{% endif %}
			</tr>
